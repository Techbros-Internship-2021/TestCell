<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/namaFungsiDiViewpager"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/lalezar"
        android:text="@string/coming_soon"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="@+id/gambarFungsiDiViewpager"
        app:layout_constraintStart_toStartOf="@+id/gambarFungsiDiViewpager"
        app:layout_constraintTop_toBottomOf="@+id/gambarFungsiDiViewpager" />

    <ImageView
        android:id="@+id/gambarFungsiDiViewpager"
        android:layout_width="235dp"
        android:layout_height="340dp"
        android:layout_marginTop="128dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/coming_soon" />
</androidx.constraintlayout.widget.ConstraintLayout>